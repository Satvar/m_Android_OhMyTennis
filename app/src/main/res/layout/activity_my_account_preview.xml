<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".activity.myaccount.MyAccountPreview">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/tool_myaccout"
        android:background="@color/colorPrimary"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/back"
            android:layout_gravity="left"
            android:src="@drawable/back_icon"
            android:layout_marginRight="20dp"
            android:layout_width="25dp"
            android:layout_height="25dp" />
        <TextView

            android:textStyle="bold"
            android:textSize="20dp"
            android:textColor="@color/white_tint"
            android:text="Mon compte"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:id="@+id/edit_tool"
            android:layout_gravity="right"
            android:src="@drawable/edit_icon"
            android:layout_marginRight="20dp"
            android:layout_width="25dp"
            android:layout_height="25dp" />
    </androidx.appcompat.widget.Toolbar>

    <ScrollView
        android:layout_marginTop="60sp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_marginTop="25dp"
                android:layout_marginRight="@dimen/login_logo_left_right"
                android:layout_marginLeft="@dimen/login_logo_left_right"
                android:layout_width="match_parent"
                android:gravity="center"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:layout_width="100dp"
                    android:layout_height="100dp">
                    <com.tech.cloudnausor.ohmytennis.utils.CircleImageView
                        android:src="@drawable/user_praba"
                        android:layout_width="100dp"
                        android:layout_height="100dp" />

                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="20dp"
                android:layout_marginLeft="@dimen/login_logo_left_right"
                android:layout_marginRight="@dimen/login_logo_left_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:src="@drawable/user_icon"
                        android:tint="@color/orgin_tint"
                        android:layout_gravity="center_vertical"
                        android:layout_width="25dp"
                        android:layout_height="25dp" />
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:textColor="#000000"

                            android:layout_marginLeft="5dp"
                            android:text="Nom"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:textSize="18dp"
                            android:layout_marginLeft="5dp"
                            android:textColorHint="#000000"
                            android:inputType="textEmailAddress"
                            android:text="Balachanran"
                            android:drawableTint="@color/orgin_tint"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:backgroundTint="@android:color/transparent"
                             />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="5dp"

                android:background="#DBDBDB"
                android:layout_width="match_parent"
                android:layout_height="2dp"/>
            <LinearLayout
                android:layout_marginTop="20dp"
                android:layout_marginLeft="@dimen/login_logo_left_right"
                android:layout_marginRight="@dimen/login_logo_left_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:src="@drawable/user_icon"
                        android:tint="@color/orgin_tint"
                        android:layout_gravity="center_vertical"
                        android:layout_width="25dp"
                        android:layout_height="25dp" />
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:textColor="#000000"

                            android:layout_marginLeft="5dp"
                            android:text="@string/mobile_hint"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:textSize="18dp"
                            android:layout_marginLeft="5dp"
                            android:inputType="textEmailAddress"
                            android:text="9500604110"
                            android:drawableTint="@color/orgin_tint"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:backgroundTint="@android:color/transparent"
                             />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="5dp"

                android:background="#DBDBDB"
                android:layout_width="match_parent"
                android:layout_height="2dp"/>

            <LinearLayout
                android:layout_marginTop="20dp"
                android:layout_marginLeft="@dimen/login_logo_left_right"
                android:layout_marginRight="@dimen/login_logo_left_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:src="@android:drawable/ic_dialog_email"
                        android:tint="@color/orgin_tint"
                        android:layout_gravity="center_vertical"
                        android:layout_width="25dp"
                        android:layout_height="25dp" />
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:textColor="#000000"

                            android:layout_marginLeft="5dp"
                            android:text="E-Mail"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:textSize="18dp"
                            android:layout_marginLeft="5dp"
                            android:inputType="textEmailAddress"
                            android:text="balavijiy@gmail.com"
                            android:drawableTint="@color/orgin_tint"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:backgroundTint="@android:color/transparent"
                             />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="5dp"

                android:background="#DBDBDB"
                android:layout_width="match_parent"
                android:layout_height="2dp"/>
            <LinearLayout
                android:layout_marginTop="20dp"
                android:layout_marginLeft="@dimen/login_logo_left_right"
                android:layout_marginRight="@dimen/login_logo_left_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:src="@drawable/ville_icon"
                        android:tint="@color/orgin_tint"
                        android:layout_gravity="center_vertical"
                        android:layout_width="25dp"
                        android:layout_height="25dp" />
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:textColor="#000000"

                            android:layout_marginLeft="5dp"
                            android:text="@string/r_ville_hint"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:textSize="18dp"
                            android:layout_marginLeft="5dp"
                            android:inputType="textEmailAddress"
                            android:text="La Possesssion"
                            android:drawableTint="@color/orgin_tint"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:backgroundTint="@android:color/transparent"
                             />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="5dp"

                android:background="#DBDBDB"
                android:layout_width="match_parent"
                android:layout_height="2dp"/>
            <LinearLayout
                android:layout_marginTop="20dp"
                android:layout_marginLeft="@dimen/login_logo_left_right"
                android:layout_marginRight="@dimen/login_logo_left_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:src="@android:drawable/ic_dialog_email"
                        android:tint="@color/orgin_tint"
                        android:layout_gravity="center_vertical"
                        android:layout_width="25dp"
                        android:layout_height="25dp" />
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:textColor="#000000"

                            android:layout_marginLeft="5dp"
                            android:text="@string/level_tennis"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:textSize="18dp"
                            android:layout_marginLeft="5dp"
                            android:inputType="textEmailAddress"
                            android:text="High"
                            android:drawableTint="@color/orgin_tint"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:backgroundTint="@android:color/transparent"
                             />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="5dp"

                android:background="#DBDBDB"
                android:layout_width="match_parent"
                android:layout_height="2dp"/>
            <LinearLayout
                android:layout_marginTop="20dp"
                android:layout_marginLeft="@dimen/login_logo_left_right"
                android:layout_marginRight="@dimen/login_logo_left_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <RelativeLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:gravity="center"
                        android:drawablePadding="5dp"
                        android:drawableLeft="@drawable/ic_email_black_24dp"
                        android:textColor="#000000"
                        android:layout_marginBottom="-6dp"
                        android:text="@string/partner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <!--<androidx.appcompat.widget.SwitchCompat-->
                        <!--android:layout_alignParentRight="true"-->

                        <!--android:layout_width="wrap_content"-->
                        <!--android:layout_height="wrap_content" />-->
                    <Switch
                        android:enabled="false"
                        android:layout_alignParentRight="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:visibility="gone"
                    android:layout_height="wrap_content"

                    android:textColor="@color/error"/>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="5dp"
                android:background="#DBDBDB"
                android:layout_width="match_parent"
                android:layout_height="2dp"/>
            <LinearLayout
                android:layout_marginTop="20dp"
                android:layout_marginLeft="@dimen/login_logo_left_right"
                android:layout_marginRight="@dimen/login_logo_left_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <RelativeLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:gravity="center"
                        android:drawablePadding="5dp"
                        android:drawableLeft="@drawable/star_icon"
                        android:textColor="#000000"
                        android:layout_marginBottom="-6dp"
                        android:text="@string/sparring"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <Switch
                        android:enabled="false"
                        android:layout_alignParentRight="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="5dp"
                android:background="#DBDBDB"
                android:layout_width="match_parent"
                android:layout_height="2dp"/>
            <LinearLayout
                android:layout_marginBottom="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="@dimen/login_logo_left_right"
                android:layout_marginRight="@dimen/login_logo_left_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:src="@drawable/bankbuild_icon"
                        android:tint="@color/orgin_tint"
                        android:layout_marginTop="10dp"
                        android:layout_width="25dp"
                        android:layout_height="25dp" />
                    <LinearLayout
                        android:layout_marginLeft="10dp"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:textColor="#000000"

                            android:layout_marginLeft="5dp"
                            android:text="@string/bank_details"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />
                        <TextView
                            android:textSize="18dp"
                            android:layout_marginLeft="5dp"
                            android:inputType="textEmailAddress"
                            android:text="ICICI Bank"
                            android:drawableTint="@color/orgin_tint"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:backgroundTint="@android:color/transparent"
                             />
                        <TextView
                            android:textSize="18dp"
                            android:layout_marginLeft="5dp"
                            android:inputType="textEmailAddress"
                            android:text="@string/user_id_hint_1"
                            android:drawableTint="@color/orgin_tint"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:backgroundTint="@android:color/transparent"
                             />
                        <TextView
                            android:textSize="18dp"
                            android:layout_marginLeft="5dp"
                            android:inputType="textEmailAddress"
                            android:text="IC00006020"
                            android:drawableTint="@color/orgin_tint"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:backgroundTint="@android:color/transparent"
                             />
                        <TextView
                            android:textSize="18dp"
                            android:layout_marginLeft="5dp"
                            android:inputType="textEmailAddress"
                            android:text="La Possesssion"
                            android:drawableTint="@color/orgin_tint"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:backgroundTint="@android:color/transparent"
                             />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="5dp"
                android:background="#DBDBDB"
                android:layout_width="match_parent"
                android:layout_height="2dp"/>
        </LinearLayout>
    </ScrollView>

</FrameLayout>